{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "dist/**", "build/**", ".medusa/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false
    },
    "db:generate": {
      "cache": true,
      "persistent": true
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "medusa:next": {}
  }
}
